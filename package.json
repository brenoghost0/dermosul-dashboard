{
  "name": "dermosul-dashboard",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "run-p \"dev:server\" \"dev:worker\" \"dev:client:wait\"",
    "dev:safe": "bash scripts/ensure-local-db.sh && npm run dev",
    "dev:server": "HOST=127.0.0.1 PORT=3008 nodemon --watch 'src/**/*.ts' --exec 'tsc -p tsconfig.server.json && node -r dotenv/config dist-server/server.js'",
    "dev:client": "vite",
    "dev:client:wait": "wait-on tcp:127.0.0.1:3008 -t 30000 && npm run dev:client",
    "dev:worker": "ts-node --project tsconfig.server.json src/workers/scrape-worker.ts",
    "build": "tsc -p tsconfig.server.json && vite build",
    "build:scripts": "tsc -p tsconfig.scripts.json",
    "start": "node dist-server/server.js",
    "worker": "node dist-server/workers/scrape-worker.js",
    "db:push": "npx prisma db push",
    "db:seed": "npx prisma db seed",
    "db:backfill-birthdate": "npx ts-node --project tsconfig.server.json scripts/backfill-birthdate.ts",
    "db:reset-seed-orders": "npx ts-node --project tsconfig.server.json scripts/reset-seed-orders.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.server.json prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@types/express-session": "^1.18.2",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.25.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dompurify": "^3.3.0",
    "express": "^4.19.2",
    "express-rate-limit": "^6.11.2",
    "express-session": "^1.18.2",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.9",
    "npm-run-all": "^4.1.5",
    "openai": "^4.76.1",
    "playwright": "^1.49.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1",
    "recharts": "^3.1.2",
    "sanitize-html": "^2.13.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@faker-js/faker": "^10.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^24.3.0",
    "@types/nodemailer": "^6.4.12",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.5",
    "@types/sanitize-html": "^2.11.4",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.14.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.1",
    "prisma": "^6.18.0",
    "tailwindcss": "^3.4.12",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.9.1",
    "vite": "^5.3.1",
    "vitest": "^4.0.1",
    "wait-on": "^9.0.3"
  }
}
